# Mycelium Chat Environment Configuration
# Copy this file to .env and configure for your deployment

# Domain Configuration
VITE_APP_DOMAIN=your-domain.com
VITE_APP_NAME=Mycelium Chat

# ThreeFold Connect Configuration
VITE_TF_CONNECT_APPID=your-domain.com
VITE_TF_CONNECT_PUBLICKEY=your-public-key
VITE_CALLBACK_URL=/src/callback.html

# Mycelium API Configuration
VITE_MYCELIUM_API_URL=http://localhost:8989

# Development Settings
VITE_DEMO_MODE=false
VITE_DEBUG_LOGGING=false

# GitHub Pages Configuration (for deployment)
VITE_BASE_URL=/
